#include<iostream>
  using namespace std;
int main(){
  string first,second;
  cin>>first>>second;
  cout<<first+second;
  return 0;
}

#include<iostream>
 using namespace std;
int main(){
  int subject[5];
  float avg=0;
  for(int i=0;i<5;i++){
    cin>>subject[i];
  }
   for(int i=0;i<5;i++){
    avg+=subject[i];
  }
  cout<<avg/2;
  return 0;
}
